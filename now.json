{
  "version": 2,
  "name": "d3594-ss",
  "regions": ["all"],
  "public": true,
  "builds": [
    { "src": "public/*", "use": "@now/static" },
    { "src": "src/index.js", "use": "@now/node@canary", "config": { "maxLambdaSize": "40mb" } }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "/(.+)", "dest": "/src/index.js" }
  ]
}
